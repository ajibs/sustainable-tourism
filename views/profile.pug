extends layout

block content
  section(class="container-fluid")
    div(class="form-container col-md-6 col-md-offset-3")
      h2= title
      div(class="account-type")
        h3(class="text-uppercase") Welcome  #{user.local.username}
        a(href="/explore")
          button(class="btn btn-primary btn-lg icon-wrapper") Book an Experience
        a(href="/create-tour")
          button(class="btn btn-success btn-lg icon-wrapper") Create an Experience